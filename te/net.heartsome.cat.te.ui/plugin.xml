      <handler
            class="net.heartsome.cat.te.ui.mergetmx.handler.MergeTmxHanlder"
            commandId="net.heartsome.cat.te.ui.mergeTmxCommand">
      </handler>
      <handler
            class="net.heartsome.cat.te.ui.tmxproperties.ShowTmxPropHandler"
            commandId="net.heartsome.cat.te.ui.command.showtmxfileprop">
         <enabledWhen>
            <test
                  property="net.heartsome.cat.te.ui.propertyTester.isFileDataAccess">
            </test>
         </enabledWhen>
      </handler>
      <handler
            class="net.heartsome.cat.te.ui.task.handler.ModifyLangCodeHandler"
            commandId="net.heartsome.cat.te.ui.command.modifyLang">
         <enabledWhen>
            <test
                  property="net.heartsome.cat.te.ui.propertyTester.isFileDataAccess">
            </test>
         </enabledWhen>
      </handler>
      <handler
            class="net.heartsome.cat.te.ui.task.handler.DeleteLangHandler"
            commandId="net.heartsome.cat.te.ui.command.deleteLang">
         <enabledWhen>
            <test
                  property="net.heartsome.cat.te.ui.propertyTester.isFileDataAccess">
            </test>
         </enabledWhen>
      </handler>
      <handler
            class="net.heartsome.cat.te.ui.task.handler.CleanTagsHandler"
            commandId="net.heartsome.cat.te.ui.command.cleanTags">
         <enabledWhen>
            <test
                  property="net.heartsome.cat.te.ui.propertyTester.isFileDataAccess">
            </test>
         </enabledWhen>
      </handler>
      <handler
            class="net.heartsome.cat.te.ui.task.handler.DeleteBatchJobsHandler"
            commandId="net.heartsome.cat.te.ui.command.batTask">
         <enabledWhen>
            <test
                  property="net.heartsome.cat.te.tmxeditor.propertyTester.isTmxEditoerEnable">
            </test>
         </enabledWhen>
      </handler>
      <handler
            class="net.heartsome.cat.te.ui.task.handler.CleanSelectedLinesTagsHandler"
