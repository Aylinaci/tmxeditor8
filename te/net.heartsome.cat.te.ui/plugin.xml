   <extension
         name="FileMenuHandler"
         point="org.eclipse.ui.handlers">
      <handler
            class="net.heartsome.cat.te.ui.splitTmx.SplitTmxHandler"
            commandId="net.heartsome.cat.te.ui.splitTmxCommand">
